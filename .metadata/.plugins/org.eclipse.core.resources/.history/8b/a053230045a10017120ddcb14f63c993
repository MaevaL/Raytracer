package tests;

import models.Ray;
import models.Sphere;
import models.Vec3;
import utils.TracingUtils;

public class TracingUtilsTest {
	
	public void intersecTest() {
		Vec3 origin = new Vec3(0,0,0);
		Vec3 direction = new Vec3(1,0,0);
		//Vec3 result = new Vec3(2,0,0);
		
		Ray r = new Ray(origin, direction);
		
		Vec3 color = new Vec3(0,0,0);
		Sphere sphere = new Sphere(origin, color, 1, false);
		
		double inter;
		inter = intersec(r, sphere);
	}

}
