public class Main {

	public static void main(String[] args) {
		Vec3Test vec = new Vec3Test();
		vec.sumTest();
		vec.multiplyByVecTest();
		vec.multiplyByNumberTest();
		vec.normeTest();
		vec.squareNormeTest();
		vec.normalizedTest();
		vec.scalarProductTest();
		vec.productVectorTest();
		
		UtilsTest utils = new UtilsTest();
		utils.clampTest();
		
		RayTest ray = new RayTest();
		ray.position3DTest();
		
		TracingUtilsTest tracing = new TracingUtilsTest();
		tracing.intersecTest();
		System.out.println("test done");

	}

}
